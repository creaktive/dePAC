  package AnyEvent::Impl::Cocoa;
  use AnyEvent ();
  BEGIN{AnyEvent::common_sense}
  use Cocoa::EventLoop;
  sub io{
  my($class,%arg)=@_;
  Cocoa::EventLoop->io(%arg)
  }
  sub timer{
  my($class,%arg)=@_;
  Cocoa::EventLoop->timer(%arg)
  }
  1
