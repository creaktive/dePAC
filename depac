  }
  sub _isa_hook($$;$){
  my($i,$pkg,$reset_ae)=@_;
  $isa_hook[$i]=$pkg?[$pkg,$reset_ae]:undef;
  _isa_set;
  }
  our@methods=qw(io timer time now now_update signal child idle condvar);
  sub detect(){
