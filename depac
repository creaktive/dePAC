  our@post_detect;
  sub post_detect(&){
  my($cb)=@_;
  push@post_detect,$cb;
  defined wantarray
    ?bless\$cb,"AnyEvent::Util::postdetect"
    :()
  }
  sub AnyEvent::Util::postdetect::DESTROY{
  @post_detect=grep$_!=${$_[0]},@post_detect;
